<section class="login_container ,clearfix">
  <div class="headline_box">
    <h2 class="login_headline">ログイン</h2>
  </div>
  <table border="1" class="login_table">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <tr>
      <div class="field">
        <th><%= f.label :メールアドレス,class: "label" %><br/></th>
        <th><%= f.email_field :email, autofocus: true, autocomplete: "email" %></th>
      </div>
    </tr>
    <tr>
      <div class="field">
        <td><%= f.label :パスワード,class: "label" %><br /></td>
        <td><%= f.password_field :password, autocomplete: "off" %></td>
      </div>
    </tr>
  </table>
  <div class="actions">
    <%= f.submit "ログイン",class: "login_btn" %>
  </div>
  <% end %>
  <%= render "users/shared/links" %>
</section>
