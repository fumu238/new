<section>
  <div>
  <h2>登録情報編集</h2>
</div>
<%= form_for(@user) do |f| %>
    <div class="">
        <label class="control-label sr-only" for="inputEmail">Name</label>
        <%= f.text_field :user_name, autofocus: true, class:"form-control", id:"inputEmail", placeholder:"名前"%>
        <span class="fa fa-user form-control-icon" aria-hidden="true"></span>
    </div>
    <%= f.submit "変更を保存", class:"" %>
<% end %>
</section>
